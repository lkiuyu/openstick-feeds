#!/bin/sh /etc/rc.common
# Init script for QMI Modem Initialization

START=90
STOP=10

USE_PROCD=1
PROG=/usr/bin/qmi-modem-410-init

start_service() {
    procd_open_instance
    procd_set_param command "$PROG"
    procd_set_param stdout 1
    procd_set_param stderr 1
    procd_close_instance
}

stop_service() {
    # Nothing to stop
    return 0
}
