#!/bin/sh /etc/rc.common
# Init script for QMI Modem Initialization

START=99
STOP=10

USE_PROCD=1

start_service() {
    procd_open_instance
    procd_set_param command /bin/sh
    procd_set_param args -c "sleep 25 && /usr/bin/qmi-modem-init"
    procd_set_param stdout 1
    procd_set_param stderr 1
    procd_close_instance
}

stop_service() {
    # Nothing to stop
    return 0
}
